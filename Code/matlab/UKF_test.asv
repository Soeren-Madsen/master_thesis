
A1=10.;
A2=20.;
A3=30.;
freq1=2.;
freq2=1.;
freq3=4.;

dt=0.1;
t=0
init_state=[10., 0., 0., A1, freq1, 0., A2, freq2, 0., A3, freq3];
ukf = unscentedKalmanFilter(@states,@measure,init_state);
for i=1:100
    t=dt*i;
    meas=A1*sin(freq1*t)+A2*sin(freq2*t)+A3*sin(freq3*t);
    correct(ukf,meas);
    predict(ukf);
    ukf.State
    mes = ukf.State(1)-ukf.State(3)-ukf.State(6)-ukf.State(9)
    gt=10
end


